{
    "bindings": [
      {
        "authLevel": "anonymous",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "methods": ["get", "post", "put", "patch", "delete", "options"],
        "route": "{*any}"
      },
      {
        "type": "http",
        "direction": "out",
        "name": "$return"
      }
    ],
    "scriptFile": "function_app.py"
  }